{
	"compilerOptions": {
		"jsx": "react",
		"module": "CommonJS",
		"target": "es6",
		"strict": true,
		"noImplicitAny": true,
		"sourceMap": true,
		"outDir": "dist",
		"strictNullChecks": true,
		"forceConsistentCasingInFileNames": true,
		"esModuleInterop": true,
		"noEmit": true,
		"resolveJsonModule": true,
		"isolatedModules": true,
		"allowJs": true,
		"skipLibCheck": true,
		"allowSyntheticDefaultImports": true,
		"moduleResolution": "node",
	},
	"exclude": [
		"node_modules/*",
		"dist/*"
	]
}
