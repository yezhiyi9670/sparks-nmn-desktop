{
	"compilerOptions": {
		"jsx": "react",
		"module": "commonjs",
		"strict": true,
		"noImplicitAny": true,
		"sourceMap": true,
		"outDir": "dist",
		"baseUrl": ".",
		"paths": {
			"*": [
				"node_modules/*"
			]
		},
		"strictNullChecks": true,
		"forceConsistentCasingInFileNames": true,
		"esModuleInterop": true
	},
	"include": [
		"src/**/*"
	],
	"exclude": [
		"dist"
	]
}
